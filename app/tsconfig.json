{
  "compilerOptions": {
    "target": "ES6",
    "lib": ["DOM", "ES6"],
    "module": "ESNext",
    "moduleResolution": "Node",
    "jsx": "react",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "baseUrl": "./frontend/react", 
    "paths": {
      "@": ["../.."]
    }
  },
  "include": ["frontend/**/*"],
  "exclude": ["node_modules", "build"]
}